{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.10",
    "@mui/material": "^6.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "clean": "cd .. && pio run --target clean",
    "build": "GENERATE_SOURCEMAP=false BUILD_PATH='../data' react-scripts build",
    "monitor": "cd .. && pio device monitor",
    "upload": "cd .. && pio run --target upload",
    "upload:monitor": "npm run upload && npm run monitor",
    "uploadfs": "cd .. && pio run --target uploadfs",
    "deploy": "npm run build && npm run uploadfs && npm run upload",
    "deploy:monitor": "npm run  deploy && npm run monitor",
    "clean:deploy": "npm run clean && npm run build && npm run uploadfs && npm run upload",
    "clean:upload": "npm run clean && npm run upload"
  },
  "eslintConfig": {
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}