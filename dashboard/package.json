{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@mui/icons-material": "^6.1.10",
    "@mui/material": "^6.1.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build": "GENERATE_SOURCEMAP=false BUILD_PATH='../data' react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pio:clean": "cd .. && pio run --target clean",
    "pio:build": "cd .. && pio run --environment ${PIO_ENV:-esp32-s3}",
    "pio:upload": "cd .. && pio run --target upload --environment ${PIO_ENV:-esp32-s3}",
    "pio:monitor": "cd .. && pio device monitor",
    "pio:uploadfs": "cd .. && pio run --target uploadfs --environment ${PIO_ENV:-esp32-s3}",
    "pio:build:monitor": "npm run pio:build && npm run pio:monitor",
    "pio:upload:monitor": "npm run pio:upload && npm run pio:monitor",
    "build:uploadfs": "npm run build && npm run pio:uploadfs",
    "build:uploadfs:monitor": "npm run build && npm run pio:uploadfs && npm run pio:monitor",
    "build:uploadfs:pio:upload:monitor": "npm run build && npm run pio:uploadfs && npm run pio:build && npm run pio:upload && npm run pio:monitor"
  },
  "eslintConfig": {
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
